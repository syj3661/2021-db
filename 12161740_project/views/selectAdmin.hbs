<h1>공항 정보</h1>

<table>
  <tr>
    <td>Airport_code</td>
    <td>Name</td>
    <td>City</td>
    <td>state</td>
  </tr>
  {{#each airport}}
  <tr>
    <td>{{Airport_code}}</td>
    <td>{{Name}}</td>
    <td>{{City}}</td>
    <td>{{state}}</td>
  </tr>
  {{/each}}
</table>

<h2>항공기 정보</h2>

<table>
  <tr>
    <td>Airplane_id</td>
    <td>Total_number_of_seats</td>
    <td>Airplane_type</td>
  </tr>
  {{#each airplane}}
  <tr>
    <td>{{Airplane_id}}</td>
    <td>{{Total_number_of_seats}}</td>
    <td>{{Airplane_type}}</td>
  </tr>
  {{/each}}
</table>

<h3>항공편 정보</h3>

<table>
  <tr>
    <td>Flight_number</td>
    <td>Leg_number</td>
    <td>Date</td>
    <td>Number_of_available_seats</td>
    <td>Airplane_id</td>
    <td>Departure_airport_code</td>
    <td>Departure_time</td>
    <td>Arrival_airport_code</td>
    <td>Arrival_time</td>
  </tr>
  {{#each leg_instance}}
  <tr>
    <td>{{Flight_number}}</td>
    <td>{{Leg_number}}</td>
    <td>{{Date}}</td>
    <td>{{Number_of_available_seats}}</td>
    <td>{{Airplane_id}}</td>
    <td>{{Departure_airport_code}}</td>
    <td>{{Departure_time}}</td>
    <td>{{Arrival_airport_code}}</td>
    <td>{{Arrival_time}}</td>
  </tr>
  {{/each}}
</table>