<h1>항공기 삭제</h1>

<table>
  <tr>
    <td>Airplane_id</td>
    <td>Total_number_of_seats</td>
    <td>Airplane_type</td>
  </tr>
  {{#each airplane}}
    <form method="post">
      <tr>
        <td>{{Airplane_id}}</td>
        <td>{{Total_number_of_seats}}</td>
        <td>{{Airplane_type}}</td>
        <td style="border: none; margin-left: 10px;">
          <button 
            style="margin-left: 10px;"
            type="submit" 
            name="total_number_of_seats"
            value={{Total_number_of_seats}} 
            formaction="/delete/adminAirplane">
          삭제
          </button>
        </td>
      </tr>    
    </form>
  {{/each}}
</table>