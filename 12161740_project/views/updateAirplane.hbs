<h1>항공기 정보 갱신</h1>

<table>
  <tr>
    <td>Airplane_id</td>
    <td>Total_number_of_seats</td>
    <td>Airplane_type</td>
  </tr>
    {{#each adminAirplane_res}}
    <form method="post" action='/update/adminAirplane'>
    <tr>
      <td>{{Airplane_id}}</td>
      <td><input type="text" name="total_number_of_seats" placeholder={{Total_number_of_seats}}></td>
      <td>{{Airplane_type}}</td>
      <td><input type="submit" value="수정"></td>
    </tr>
    </form>
    {{/each}}
</table>