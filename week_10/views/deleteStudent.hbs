<h1>{{ title }}</h1>

<table>
  <tr>
    <td>Sname</td>
    <td>Snumber</td>
  </tr>
  {{#each student}}
    <form method="post">
      <tr>
        <td>{{Sname}}</td>
        <td>{{Snumber}}</td>
        <td style="border: none; margin-left: 10px;">
          <button 
            style="margin-left: 10px;"
            type="submit" 
            name="snumber"
            value={{Snumber}} 
            formaction="/delete/student">
          삭제
          </button>
        </td>
      </tr>    
    </form>
  {{/each}}
</table>